{
  "manifest_version": 2,
  "name": "Amazon Link Cleaner",
  "version": "2.2",
  "description": "Remove unnecessary parts from Amazon URLs",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.amazon.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
